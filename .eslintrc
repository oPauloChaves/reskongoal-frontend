{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "plugins": [
    "babel"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "semi": [2, "never"],
    "max-len": [2, 120, 2],
    "generator-star-spacing": 0,
    "jsx-quotes": ["error","prefer-single"],
    "react/prop-types": "off",
    "space-before-function-paren": "off",
    "padded-blocks": "off"
  }
}
